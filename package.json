{
  "name": "npm-navigator",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "module": "src/main.js",
  "description": "navigate npm dependencies visually",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "semantic-release": "semantic-release",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "docs": "documentation readme src/main.js --section=API",
    "lint": "documentation lint src/main.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "WiredPanels": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.8",
    "documentation": "^8.1.2",
    "koa": "^2.5.2",
    "koa-static": "^5.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.1.0",
    "puppeteer": "^1.8.0",
    "rollup": "^0.66.4",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9",
    "rollup-plugin-json": "^3.1.0"
  },
  "engines": {
    "node": ">=10.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-navigator/issues"
  },
  "homepage": "https://github.com/arlac77/npm-navigator#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
