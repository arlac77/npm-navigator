{
  "name": "npm-navigator",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "module": "src/main.js",
  "description": "navigate npm dependencies visually",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/main.js --section=API",
    "lint": "documentation lint src/main.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "WiredPanels": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^6.0.0",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "koa": "^2.10.0",
    "koa-static": "^5.0.0",
    "markdown-doctest": "^1.0.0",
    "puppeteer": "^2.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-fill-html": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.28"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-navigator/issues"
  },
  "homepage": "https://github.com/arlac77/npm-navigator#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
