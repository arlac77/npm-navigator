{
  "name": "npm-navigator",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/main.js",
  "module": "src/main.js",
  "description": "navigate npm dependencies visually",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme dist/main.js --section=API",
    "lint": "documentation lint dist/main.js"
  },
  "dependencies": {
    "WiredPanels": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^3.8.2",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.2",
    "documentation": "^13.0.0",
    "koa": "^2.11.0",
    "koa-static": "^5.0.0",
    "puppeteer": "^3.0.2",
    "rollup": "^2.8.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.6.0",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-navigator/issues"
  },
  "homepage": "https://github.com/arlac77/npm-navigator#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-cli-app",
      "arlac77/template-github"
    ]
  }
}
